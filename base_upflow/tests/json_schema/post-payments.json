{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$comment": "source: https://upflow.docs.apiary.io/#reference/0/payments/import-payment",
  "type": "object",
  "properties": {
    "currency": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "enum": ["EUR", "CHF", "USD", "CAD", "GBP"]
        }
      ]
    },
    "amount": {
      "type": "number"
    },
    "instrument": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "enum": ["WIRE_TRANSFER", "DIRECT_DEBIT", "CARD", "CASH", "CHECK", "UNKNOWN"]
        }
      ]
    },
    "validatedAt": {
      "type": "string"
    },
    "externalId": {
      "type": "string"
    },
    "bankAccountId": {
      "type": "string"
    },
    "linkedInvoices": {
      "type": "array"
    },
    "customer": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "externalId": {
          "type": "string"
        }
      }
    }
  },
  "required": ["currency", "amount", "validatedAt"]
}
