#. module: account_invoice_overdue_reminder
#: model:mail.template,body_html:account_invoice_overdue_reminder.overdue_invoice_reminder_mail_template
msgid ""
"\n"
"<div style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"<p>Dear customer,</p>\n"
"\n"
"<p>According to our books, the following invoices are overdue:</p>\n"
"\n"
"<table style=\"border-spacing: 0; border-collapse: collapse; width: 100%; "
"text-align: center;\">\n"
"    <tr>\n"
"        <th style=\"padding: 5px; border: 1px solid black;\">Invoice Number</"
"th>\n"
"        <th style=\"padding: 5px; border: 1px solid black;\">Invoice Date</"
"th>\n"
"        <th style=\"padding: 5px; border: 1px solid black;\">Payment Terms</"
"th>\n"
"        <th style=\"padding: 5px; border: 1px solid black;\">Due Date</th>\n"
"        <th style=\"padding: 5px; border: 1px solid black;\">Order Ref.</"
"th>\n"
"        <th style=\"padding: 5px; border: 1px solid black;\">Type</th>\n"
"        <th style=\"padding: 5px; border: 1px solid black;\">Total Untaxed</"
"th>\n"
"        <th style=\"padding: 5px; border: 1px solid black;\">Total</th>\n"
"        <th style=\"padding: 5px; border: 1px solid black;\">Residual</th>\n"
"        <th style=\"padding: 5px; border: 1px solid black;\">Past Reminders</"
"th>\n"
"    </tr>\n"
"% for inv in object.invoice_ids.sorted(key='date_invoice'):\n"
"    <tr style=\"background-color:\n"
"        % if inv.type == 'out_refund':\n"
"            LightGray\n"
"        % endif\n"
"    \">\n"
"        <td style=\"padding: 5px; border: 1px solid black;\">${inv.number}</"
"td>\n"
"        <td style=\"padding: 5px; border: 1px solid black;\">"
"${format_date(inv.date_invoice)}</td>\n"
"        <td style=\"padding: 5px; border: 1px solid black;\">${inv."
"payment_term_id.name or ''}</td>\n"
"        <td style=\"padding: 5px; border: 1px solid black;\">"
"${format_date(inv.date_due)}</td>\n"
"        <td style=\"padding: 5px; border: 1px solid black;\">${inv.name or "
"''}</td>\n"
"        <td style=\"padding: 5px; border: 1px solid black;\">${dict(inv."
"fields_get(allfields=['type'])['type']['selection'])[inv.type]}</td>\n"
"        <td style=\"padding: 5px; border: 1px solid black; text-align: right;"
"\">${format_amount(inv.amount_untaxed_invoice_signed, inv.currency_id)}</"
"td>\n"
"        <td style=\"padding: 5px; border: 1px solid black; text-align: right;"
"\">${format_amount(inv.amount_total_signed, inv.currency_id)}</td>\n"
"        <td style=\"padding: 5px; border: 1px solid black; text-align: right;"
"\">${format_amount(inv.residual_signed, inv.currency_id)}</td>\n"
"        <td style=\"padding: 5px; border: 1px solid black;\">${inv."
"overdue_reminder_counter}</td>\n"
"    </tr>\n"
"% endfor\n"
"% for (currency, total_residual) in object.total_residual():\n"
"    <tr>\n"
"        <td></td>\n"
"        <td></td>\n"
"        <td></td>\n"
"        <td></td>\n"
"        <td></td>\n"
"        <td></td>\n"
"        <td colspan=\"2\" style=\"padding: 5px; border: 1px solid black; "
"font-weight: bold; text-align: right;\">Total Residual in ${currency.name}:</"
"td>\n"
"        <td style=\"padding: 5px; border: 1px solid black; font-weight: "
"bold; text-align: right;\">${format_amount(total_residual, currency)}</td>\n"
"        <td></td>\n"
"    </tr>\n"
"% endfor\n"
"</table>\n"
"\n"
"<p>If you made a payment for these invoices a few days ago, please ignore "
"this email.</p>\n"
"\n"
"% if object.company_id.overdue_reminder_attach_invoice:\n"
"<p>You will find enclosed the overdue invoices.</p>\n"
"% endif\n"
"\n"
"% if object.counter > 2:\n"
"<p>Despite several reminders, we are disappointed to see that these overdue "
"invoices are still unpaid. In order to avoid legal proceedings, we urge you "
"to paid these overdue invoices in the next days.</p>\n"
"% endif\n"
"\n"
"<p>Regards,</p>\n"
"\n"
"</div>\n"
msgstr ""

