<odoo>

    <record model="ir.ui.view" id="credit_line_emailer_form_view">
        <field name="name">credit.line.emailer.form (in account_credit_control_queue_job)</field>
        <field name="model">credit.control.emailer</field>
        <field name="inherit_id" ref="account_credit_control.credit_line_emailer_form"/>
